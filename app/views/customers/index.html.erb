<h1>All Customers</h1>
<table>
    <tr>
        <th>Name</th>
        <th>Address</th>
    </tr>
    <% @customers.each do |customer| %>
        <tr>
            <td>
                <%= customer.name %>
            </td>
            <td>
                <%= customer.address %>
            </td>
        </tr>
    <% end %>
</table>